{
    "name": "spotisearch",
    "version": "0.0.1",
    "description": "Searches for tracks in your Spotify playlists",
    "main": "index.js",
    "scripts": {
        "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
        "publish": "aws s3 rm --recursive s3://spotify-search-yarrow-su && aws s3 cp --recursive dist s3://spotify-search-yarrow-su",
        "dev": "cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js",
        "lint": "npm-run-all -c lint:*",
        "lint:eslint": "eslint src/**/*.{js,svelte}",
        "lint:stylelint": "stylelint src/**/*.scss",
        "lint:prettier": "prettier --check src/**/*.{js,scss}",
        "format": "npm-run-all -c format:*",
        "format:eslint": "eslint --fix src/**/*.{js,svelte}",
        "format:stylelint": "stylelint --fix src/**/*.scss",
        "format:prettier": "prettier --write src/**/*.{js,scss,svelte}",
        "prepare": "husky install"
    },
    "keywords": [
        "spotify",
        "search",
        "playlist"
    ],
    "author": "Alex Bukharov <alexv@livewire.cc>",
    "license": "ISC",
    "engines": {
        "node": "^20.12.2"
    },
    "browserslist": [
        "last 2 versions"
    ],
    "lint-staged": {
        "*.svelte": [
            "npm run lint:prettier"
        ],
        "*.js": [
            "npm run lint:eslint"
        ],
        "*.scss": [
            "npm run lint:stylelint"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@sveltestrap/sveltestrap": "^6.2.7",
        "autoprefixer": "^10.4.19",
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.1",
        "eslint": "^9.1.1",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-svelte": "^2.38.0",
        "favicons-webpack-plugin": "^6.0.1",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.2",
        "mini-css-extract-plugin": "^2.9.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.3",
        "process": "^0.11.10",
        "promise-throttle": "^1.1.2",
        "ress": "^5.0.2",
        "rimraf": "^5.0.5",
        "sass": "^1.75.0",
        "sass-loader": "^14.2.1",
        "sitemap-webpack-plugin": "^1.1.1",
        "spotify-web-api-js": "^1.5.2",
        "style-loader": "^4.0.0",
        "stylelint": "^16.4.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-scss": "^6.2.1",
        "svelte": "^4.2.15",
        "svelte-loader": "^3.2.0",
        "svelte-preprocess": "^5.1.4",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-remove-empty-scripts": "^1.0.4"
    },
    "dependencies": {
        "svelte-spa-router": "^4.0.1"
    }
}
